            Flag disabling steering on first received LU in country
            <option value="0">
               STEERING
            </option>
            <option value="1">
               NOSTEERING
            </option>
         </parameter>
         <parameter name="FLAG-USE-MAP3" mandatory="0" default="1" type="select">
            Flag enabling MAP3 access (last IMEI, user type, etc)
            <option value="0">
               OFF
            </option>
            <option value="1">
               ON
            </option>
         </parameter>
         <parameter name="FLAG-USE-TRIGGER" mandatory="0" default="1" type="select">
            Flag enabling EIR Trigger (return to Home PLMN)
            <option value="0">
               OFF
            </option>
            <option value="1">
               ON
            </option>
         </parameter>
         <parameter name="WELCOME-ENABLED" mandatory="0" default="0" type="select">
            Flag enabling Welcome SMS service
            <option value="0">
               OFF
            </option>
            <option value="1">
               ON
            </option>
         </parameter>
         <parameter name="MAP3-PSEND-TIMEOUT" mandatory="0" default="5" type="numeric" minvalue="1" maxvalue="60">
