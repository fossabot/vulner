      <section type="common" name="GTT">
         GTT parameters
         <parameter name="TASK-ID" mandatory="1" type="numeric" minvalue="1" maxvalue="32767">
            GTT INS task id
         </parameter>
         <parameter name="SERVER-CLASS" mandatory="1" type="numeric" minvalue="1" maxvalue="32767">
            GTT INS server class
         </parameter>
         <parameter name="MGT" mandatory="1" type="path">
            GTT Mobile Global Titles db
         </parameter>
         <parameter name="MGT-BY-RANGE" mandatory="1" default="0" type="select">
            GTT flag enabling MGT by range management
            <option value="0">
               PREFIX
            </option>
            <option value="1">
               RANGE
            </option>
         </parameter>
         <parameter name="SSN-TYPES" mandatory="1" type="char" maxlength="255">
            GTT SSN Types
         </parameter>
         <parameter name="PROCESS" mandatory="1" type="char" maxlength="6">
            GTT Web Command Server process name
         </parameter>
         <parameter name="COMMANDS" mandatory="1" type="char" maxlength="255">
            GTT Web commands
         </parameter>
         <parameter name="PC-FORMATS" mandatory="1" type="char" maxlength="255">
            GTT Point Code Formats
         </parameter>
         <parameter name="NETWORK-NODES" mandatory="1" type="path">
            GTT Network Nodes db
         </parameter>
         <parameter name="LOG-DAYS" mandatory="1" default="8" type="numeric" minvalue="0" maxvalue="30">
            LOG Days
         </parameter>
         <parameter name="LOG-LEVEL" mandatory="1" default="9" type="select">
            LOG Level
            <option value="1">
               ERROR
            </option>
            <option value="5">
               WARNING
            </option>
            <option value="9">
               INFO
            </option>
            <option value="10">
               DEBUG
            </option>
            <option value="11">
               DEBUG2
            </option>
         </parameter>
         <parameter name="START-DELAY" mandatory="1" default="1" type="numeric" minvalue="1" maxvalue="10">
            GTT Start Delay (seconds)
         </parameter>
         <parameter name="STAT-TIMEOUT" mandatory="1" default="5" type="numeric" minvalue="5" maxvalue="15">
            GTT Statistics Interval (minutes)
         </parameter>
         <parameter name="STAT-GROUP" mandatory="1" default="38" type="numeric" minvalue="1" maxvalue="32767">
            GTT Statistics Group
         </parameter>
         <parameter name="STAT-REGSET" mandatory="1" default="GTTS-STAT" type="char" maxlength="15">
            GTT Statistics Register Set
         </parameter>
         <parameter name="STAT-MAXREGS" mandatory="1" default="100" type="numeric" minvalue="1" maxvalue="1024">
            GTT Statistics Max Register number
         </parameter>
         <parameter name="LOG-PREFIX" mandatory="0" type="char" maxlength="6">
            LOG Prefix (instead of default process name)
         </parameter>
      </section>
