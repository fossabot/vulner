      <section type="common" name="EMS">
         Event Management System parameters
         <parameter name="EMS-OWNER" mandatory="1" default="TIM" type="char" maxlength="8">
            EMS Owner
         </parameter>
         <parameter name="EMS-VERSION" mandatory="1" default="A09" type="char" maxlength="3">
            EMS Version
         </parameter>
         <parameter name="EMS-APPL" mandatory="1" default="TFS" type="char" maxlength="10">
            EMS Application Name
         </parameter>
         <parameter name="EMS-SUBSYSTEM" mandatory="1" default="760" type="numeric" minvalue="0" maxvalue="32767">
            EMS Subsystem
         </parameter>
         <parameter name="EMS-TIME-INTERVAL" mandatory="0" default="60" type="numeric" minvalue="1" maxvalue="86400">
            EMS Time Interval (seconds) for same event
         </parameter>
      </section>
