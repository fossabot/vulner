         <parameter name="DB-LOC-THRESHOLDS-PATH" mandatory="1" type="path">
            Local Steering Rules db (i.e. Thresholds)
         </parameter>
         <parameter name="DB-LOC-COUNTRIES-PATH" mandatory="1" type="path">
            Local Countries db
         </parameter>
         <parameter name="DB-LOC-OPER-PATH" mandatory="1" type="char" maxlength="9999">
            Local Operators db
         </parameter>
         <parameter name="DB-LOC-OPERGT-PATH" mandatory="1" type="path">
            Local Operator Global Titles db
         </parameter>
         <parameter name="DB-LOC-NOSTD-TAC-PATH" mandatory="1" type="path">
            Local Not Standard TAC db
         </parameter>
         <parameter name="DB-LOC-PSRULES-PATH" mandatory="1" type="path">
            Local PreSteering Rules db (e.g. Operators black list)
         </parameter>
         <parameter name="DB-REM-PSRULES-PATH" mandatory="1" type="path">
            Remote PreSteering Rules db (e.g. Operators black list)
         </parameter>
         <parameter name="DB-REM-THRESHOLDS-PATH" mandatory="1" type="path">
            Remote Steering Rules db (i.e. Thresholds)
         </parameter>
         <parameter name="DB-REM-COUNTRIES-PATH" mandatory="1" type="path">
            Remote Countries db
         </parameter>
         <parameter name="DB-REM-OPER-PATH" mandatory="1" type="path">
            Remote Operators db
         </parameter>
         <parameter name="DB-REM-OPERGT-PATH" mandatory="1" type="path">
            Remote Operator Global Titles db
         </parameter>
         <parameter name="DB-REM-NOSTD-TAC-PATH" mandatory="1" type="path">
            Remote Not Standard TAC db
         </parameter>
         <parameter name="IMSI-GSM-MBE-PATH" mandatory="1" type="path">
            IMSI GSM Users db
         </parameter>
         <parameter name="IMSI-DAT-MBE-PATH" mandatory="1" type="path">
            IMSI DAT Users db (i.e. EDGE, GPRS, HSDP)
         </parameter>
         <parameter name="IMSI-LTE-MBE-PATH" mandatory="1" type="path">
            IMSI LTE Users db
         </parameter>
         <parameter name="MBE-NOWAIT-TIMEOUT" mandatory="0" default="2" type="numeric" minvalue="1" maxvalue="10">
            Memory Based Enscribe timeout (seconds) for nowait use
         </parameter>
         <parameter name="APPLYST" mandatory="1" type="oss_path">
            Holds last steering configuration reload
         </parameter>
         <parameter name="APPLYPS" mandatory="1" type="oss_path">
            Holds last pre-steering configuration reload
         </parameter>
         <parameter name="CAR-TABLE-OSS" mandatory="1" type="oss_path">
            Operator characteristics definition file
         </parameter>
         <parameter name="BLBO-ARP-MBE-PATH" mandatory="0" type="path">
            Black List LBO and/or ARP Users db
         </parameter>
         <parameter name="ROAMING-EU-IF4-PATH" mandatory="0" type="path">
            ARP IF4 Notifications queue
         </parameter>
      </section>
      <section type="common" name="ALIGN">
