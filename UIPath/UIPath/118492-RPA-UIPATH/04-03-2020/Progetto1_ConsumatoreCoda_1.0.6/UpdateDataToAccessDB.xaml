    <ui:ForEachRow CurrentIndex="{x:Null}" DataTable="[inputDT]" DisplayName="For Each Row - Estrazione dati per aggiornamento entry sul DB" sap2010:WorkflowViewState.IdRef="ForEachRow_2">
